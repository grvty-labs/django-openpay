    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "clean-webpack-plugin": "^0.1.13",
    "css-loader": "^0.23.1",
    "deep-equal": "^1.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "jquery": "^3.1.1",
    "lodash": "^4.16.4",
    "node-sass": "^3.13.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "resolve-url-loader": "^1.4.3",
    "sass-loader": "^4.0.0",
    "script-loader": "^0.7.0",
    "source-list-map": "^0.1.6",
    "source-map": "^0.5.6",
    "style-loader": "^0.13.1"
  },
  "dependencies": {
    "webpack": "^1.13.0",
    "webpack-bundle-tracker": "^0.0.93"
  }
